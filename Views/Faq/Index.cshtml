@using _2021_dotnet_g_28.Models.Viewmodels;

@model FaqIndexViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>FAQ</h1>


<table class="table">
    <thead>
        <tr>
            <th>Probleem</th>
            <th>Oplossing</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Faqs)
        {
            <tr>
                <td>
                    @item.Problem
                </td>
                <td>
                    @item.Solution
                </td>
            </tr>
        }
    </tbody>
</table>
